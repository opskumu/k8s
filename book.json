{
    "structure": {
        "readme": "todo.md"
    },
    "plugins": [
        "-sharing",
        "github",
        "anchors",
        "puml",
        "alerts",
        "splitter",
        "todo",
        "anchor-navigation-ex",
        "expandable-chapters",
        "theme-comscore",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": false,
            "multipleH1": true,
            "float": {
                "showLevelIcon": true,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "github": {
            "url": "https://github.com/opskumu"
        },
        "theme-default":{
            "showLevel": true
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy; 2019 <a href=\"https://opskumu.com\">Kumu</a>",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}
